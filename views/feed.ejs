<% include ./partials/header.ejs %>
<div class="w-full min-h px-10 py-8 ">
  <div class="cards flex flex-wrap gap-10">
    <% posts.forEach(function(elem){ %>
        <div class="w-32">
           <div class="card w-full h-40  rounded-lg overflow-hidden">
               <img class="w-full h-full object-cover"src="/images/uploads/<%= elem.image %>" alt="">
                
           </div>
           <h5 class="mt-2 text-sm font-semibold"><%= elem.title %></h5>
           <h6 class="text-x5 opacity-500 ">@<%= elem.user.username %></h6>
        </div>
     <% }) %>
  </div>
</div>

<% include ./partials/footer.ejs %>